include ../config.mk

all: ${TARGET}

${TARGET}:  ${OBJECTS}
	${AR} cr $@ ${OBJECTS}
	ranlib $@

clean:                                                                          
	rm -f *.bak *.o *~ *% core ${TARGET}

dep:
	mv -f Makefile Makefile.bak; \
	sed -e '/^# DO NOT DELETE/,$$d' Makefile.bak > Makefile; \
	echo '# DO NOT DELETE THIS LINE - MAKE DEP RELIES ON IT' >> Makefile; \
	echo -e '# dependencies generated by make dep\n' >> Makefile; \
	for f in *.cpp; do \
		$(CXX) -MM -I../${INCLUDEDIR} $$f >> Makefile; \
		echo >> Makefile; \
	done; \
	echo -e '\n# end of generated dependencies' >> Makefile

# DO NOT DELETE THIS LINE - MAKE DEP RELIES ON IT
# dependencies generated by make dep

